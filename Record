import java.util.*;
import java.io.*;

public class Record {

	List<Field> listFields;
	Record()
	{
		listFields = new ArrayList<>();
	}
	public void Add(Field pfield)
	{
		listFields.add(pfield);
	}
	public void Print()
	{
		for(int i=0; i<listFields.size(); i++)
		{
			listFields.get(i).Print();
		}
	}
};
